{
  "name": "ykw/cruet",
  "description": "PHP port of the Rust cruet library case conversion functions. Provides camelCase, snake_case, kebab-case, PascalCase and other case transformations.",
  "type": "library",
  "keywords": [
    "string",
    "case",
    "conversion",
    "camelcase",
    "snake_case",
    "kebab-case",
    "pascalcase",
    "screaming-snake-case",
    "train-case",
    "title-case",
    "sentence-case"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "PHP Cruet Contributors",
      "email": "contributors@example.com"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Ykw\\Cruet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ykw\\Cruet\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage --coverage-clover coverage/clover.xml"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
